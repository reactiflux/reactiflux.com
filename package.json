  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "prettier --check 'src/**/*.{js,md}'",
    "prettier": "prettier --write 'src/**/*.{js,md}'",
    "install-if-package-changed": "git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD | grep --quiet yarn.lock && yarn install || exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-merge": "yarn install-if-package-changed",
      "post-checkout": "yarn install-if-package-changed"
    }
  },
  "dependencies": {
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/styled-components": "^5.1.15",
    "@vcarl/remark-headings": "^0.0.1",
    "date-fns": "^2.27.0",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "gray-matter": "^4.0.3",
    "husky": "^4.2.5",
    "minireset.css": "^0.0.6",
    "moment": "^2.27.0",
    "next": "12.0.4",
    "polished": "^3.6.5",
    "prettier": "^2.1.0",
    "pretty-quick": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-headroom": "^2.1.2",
    "react-helmet": "^5.2.1",
    "rehype-slug": "^5.0.0",
    "rehype-stringify": "^9.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "styled-components": "^5.3.3",
    "typeface-poppins": "^0.0.72",
    "typeface-space-mono": "^0.0.71",
    "typeface-work-sans": "^0.0.72",
    "typescript": "4.5.2",
    "unified": "^10.1.1"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
